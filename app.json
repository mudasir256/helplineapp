{
  "expo": {
    "name": "Helpline Welfare Trust",
    "slug": "helpline-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "helpline-app",
    "splash": {
      "image": "./assets/images/logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helpline.app",
      "statusBar": {
        "barStyle": "light-content"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "helpline-app",
              "com.helpline.app",
              "com.googleusercontent.apps.909435925564-n4ltt9ao229hf7j0iqg4okb5bbhfcnhh"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3498db"
      },
      "package": "com.helpline.app",
      "statusBar": {
        "backgroundColor": "#3498db",
        "barStyle": "light-content"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.909435925564-n4ltt9ao229hf7j0iqg4okb5bbhfcnhh"
        }
      ]
    ],
    "extra": {
      "googleWebClientId": "${EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID}"
    }
  }
}
